 {\n    for (let value of tiles.getTilesByType(sprites.dungeon.collectibleInsignia)) {\n        list = randint(1, 2)\n        if (list == 1) {\n            coin1Sprite = sprites.create(assets.image`coin1`, SpriteKind.coin1)\n            tiles.placeOnTile(coin1Sprite, value)\n        } else {\n            coin2Sprite = sprites.create(assets.image`coin2`, SpriteKind.coin2)\n            tiles.placeOnTile(coin2Sprite, value)\n        }\n    }\n}\ncontroller.left.onEvent(ControllerButtonEvent.Pressed, function () {\n    characterAnimations.loopFrames(\n    mySprite,\n    assets.animation`playerWalkLeft`,\n    200,\n    characterAnimations.rule(Predicate.MovingLeft)\n    )\n})\ncontroller.right.onEvent(ControllerButtonEvent.Pressed, function () {\n    characterAnimations.loopFrames(\n    mySprite,\n    assets.animation`playerWalkRight`,\n    200,\n    characterAnimations.rule(Predicate.MovingRight)\n    )\n})\ncontroller.down.onEvent(ControllerButtonEvent.Pressed, function () {\n    characterAnimations.loopFrames(\n    mySprite,\n    assets.animation`playerWalkDown`,\n    200,\n    characterAnimations.rule(Predicate.MovingDown)\n    )\n})\nfunction createPlayerSprite () {\n    mySprite = sprites.create(assets.image`Player`, SpriteKind.Player)\n}\nlet coin2Sprite: Sprite = null\nlet coin1Sprite: Sprite = null\nlet list = 0\nlet projectile: Sprite = null\nlet mySprite: Sprite = null\ntiles.setCurrentTilemap(tilemap`level1`)\ncreatePlayerSprite()\ntiles.placeOnRandomTile(mySprite, sprites.dungeon.doorOpenSouth)\ncontroller.moveSprite(mySprite, 50, 50)\nscene.cameraFollowSprite(mySprite)\nspawnCoins()\n","README.md":" ","assets.json":"","tilemap.g.jres":"{\n    \"transparency16\": {\n        \"data\": \"hwQQABAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA==\",\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"tilemapTile\": true\n    },\n    \